<% include partials/header.ejs %> <!-- Header of tha page -->

<%-include partials/nav.ejs%> <!-- Nav of the page --> 


<div class="container" id="contact">
    <div class="row justify-content-center text-center">
        <div class="col-12 " style="margin-top: 40px;">
            <h1>Contact</h1>
        </div>
        <div class="col-12 text-center">
            <h3 style="color:#04BF9D;">Have a question or want to work together?</h3>
        </div>
        <div class="col-md-6 col-sm-12">
            <div class="input-group mb-3">
                <input type="text" class="form-control" placeholder="First name" aria-label="fname" name="fname" id="fnameText" aria-describedby="basic-addon1" required>
            </div>
           
            <div class="input-group mb-3">
                <input type="text" class="form-control" placeholder="Last name" aria-label="lname" name="lname" id="lnameText" aria-describedby="basic-addon1" required>
            </div>

            <div class="input-group mb-3">
                <input type="tel" class="form-control" placeholder="Phone number" aria-label="phone" name="phonr" id="PhoneNumber" aria-describedby="basic-addon1" required>
            </div>

            <div class="input-group mb-3">
                <input type="email" class="form-control" placeholder="Mail" aria-label="Mail" name="mail" id="mailText" aria-describedby="basic-addon1" required>
            </div>

            <div class="input-group">
                <textarea class="form-control" aria-label="With textarea" placeholder="Your message" name="message" id="messageText" required></textarea>
            </div>
            <a href="/"><input type="submit" value="Submit" style="width: 50%; margin-bottom: 20px;" class="btn btn-primary greenBtn
                this.stystyle.borderColononmouseoutrimary" onmouseover="this.style.backgroundColor='#03A696'; this.style.borderColor='#03A696';" onmouseout="this.style.backgroundColor='#ffff'"></a>
        </div>
    </div>
</div>

<%-include partials/footer.ejs%> <!--Footer of the page-->
